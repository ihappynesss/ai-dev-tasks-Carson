{
  "title": "NSW Strata Business Metrics (Tasks 12.6-12.11)",
  "uid": "business-metrics",
  "tags": ["business", "tickets", "automation", "costs", "task-12.6-12.11"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "1m",
  "time": {"from": "now-24h", "to": "now"},
  "panels": [
    {
      "id": 1,
      "title": "Tickets Processed (Hourly)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [{
        "expr": "sum(tickets_processed_hourly) by (category)",
        "legendFormat": "{{category}}"
      }]
    },
    {
      "id": 2,
      "title": "Tickets Processed (Daily)",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [{
        "expr": "sum(tickets_processed_daily)",
        "legendFormat": "Total Daily"
      }]
    },
    {
      "id": 3,
      "title": "Automation Rate by Category",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [{
        "expr": "automation_rate",
        "legendFormat": "{{category}}"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 0.3},
              {"color": "green", "value": 0.5}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Average Similarity Scores",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [{
        "expr": "similarity_score_avg",
        "legendFormat": "{{category}}"
      }]
    },
    {
      "id": 5,
      "title": "CSAT Scores by Category",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [{
        "expr": "csat_score",
        "legendFormat": "{{category}} - {{automation_level}}"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 1,
          "max": 5,
          "thresholds": {
            "steps": [
              {"color": "red", "value": 1},
              {"color": "yellow", "value": 3},
              {"color": "green", "value": 4}
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Resolution Time by Category",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [{
        "expr": "resolution_time_avg_seconds / 3600",
        "legendFormat": "{{category}} - {{automation_level}}"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "h"
        }
      }
    },
    {
      "id": 7,
      "title": "API Cost per Ticket (USD)",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [{
        "expr": "api_cost_per_ticket_usd",
        "legendFormat": "{{category}}"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 1.5},
              {"color": "red", "value": 2.0}
            ]
          }
        }
      }
    },
    {
      "id": 8,
      "title": "API Rate Limit Consumed %",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [{
        "expr": "api_rate_limit_consumed_percent",
        "legendFormat": "{{provider}}"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      }
    }
  ]
}
