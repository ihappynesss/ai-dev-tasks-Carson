    {
      "parameters": {
        "url": "http://prometheus:9090/api/v1/query_range",
        "method": "POST",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "query",
              "value": "sum(rate(n8n_workflow_executions_total{status=\"error\"}[1d])) / sum(rate(n8n_workflow_executions_total[1d]))"
            },
            {
              "name": "start",
              "value": "={{ $now.minus({months: 1}).toUnixInteger() }}"
            },
