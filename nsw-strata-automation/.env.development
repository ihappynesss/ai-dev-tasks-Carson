# Development Environment Configuration
# For local Docker development and testing

# Environment
NODE_ENV=development
ENVIRONMENT=development

# PostgreSQL Database
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n_dev_password
POSTGRES_DB=n8n_dev
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n_dev
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=n8n_dev_password

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB_QUEUE=0
REDIS_DB_CACHE=1
REDIS_DB_RATELIMIT=2
REDIS_DB_FAILED=3

# n8n Authentication (weak credentials for dev only)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/

# Execution Mode (regular for local dev - faster feedback)
EXECUTIONS_MODE=regular
EXECUTIONS_PROCESS=main
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

# Queue Configuration (disabled in dev mode)
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_DB=0
QUEUE_BULL_REDIS_PASSWORD=
QUEUE_HEALTH_CHECK_ACTIVE=false
QUEUE_RECOVERY_INTERVAL=60

# Timezone
GENERIC_TIMEZONE=Australia/Sydney

# Logging (verbose for debugging)
N8N_LOG_LEVEL=debug
N8N_LOG_OUTPUT=console,file

# Development Features
N8N_PERSONALIZATION_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false
N8N_TEMPLATES_ENABLED=true
N8N_METRICS=false

# Security (relaxed for dev)
N8N_SECURE_COOKIE=false

# Performance (lower limits for dev)
EXECUTIONS_TIMEOUT=300
EXECUTIONS_TIMEOUT_MAX=600
