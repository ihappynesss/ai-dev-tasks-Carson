# n8n Docker Development Environment Variables
# Copy this file to .env and update with your values

# PostgreSQL Database
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n_password
POSTGRES_DB=n8n

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB_QUEUE=0
REDIS_DB_CACHE=1
REDIS_DB_RATELIMIT=2
REDIS_DB_FAILED=3

# n8n Authentication
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/

# Timezone
GENERIC_TIMEZONE=Australia/Sydney

# Execution Mode (use 'regular' for local dev, 'queue' for production-style testing)
EXECUTIONS_MODE=regular

# Queue Configuration (only used when EXECUTIONS_MODE=queue)
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_DB=0
QUEUE_BULL_REDIS_PASSWORD=
QUEUE_HEALTH_CHECK_ACTIVE=true
QUEUE_RECOVERY_INTERVAL=60

# Logging
N8N_LOG_LEVEL=info
